
# Do not edit this file below this point. Any changes will be overwritten.
.PHONY: docker wait wait-db

docker:
	mkdir -p docker/.build
	git archive --format tar.gz -o docker/.build/context.tar.gz master
	docker build -t orchestrationdev/<%= config.fetch(:app_id) %> ./docker/

wait: wait-db

wait-db:
	bundle exec rake orchestration:orchestration:db:wait
